  "author": "rioam2",
  "homepage": "https://github.com/rioam2/ProxC#readme",
  "description": "ProxC allows you to create extensible classes using ES6 Proxy objects and design declarative APIs that are a joy to use.",
  "main": "./dist/src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rioam2/ProxC.git"
  },
  "bugs": {
    "url": "https://github.com/rioam2/ProxC/issues"
  },
  "keywords": [
    "npm-package",
    "nodejs",
    "api-wrapper",
    "typescript",
    "operator",
    "overloading",
    "javascript",
    "functors",
    "index-operator",
    "bracket-operator",
    "setters",
    "getters"
  ],
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "pretest": "yarn build",
    "test": "nyc mocha -s 0 --reporter spec dist/test/**/*.spec.js",
    "watch": "mocha -s 0 --require ts-node/register --watch-extensions ts --watch test/**/*.spec.ts",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3333"
  }
}
